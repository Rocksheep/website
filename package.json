    "laravel-mix-versionhash": "^1.0",
    "lodash.throttle": "^4.1",
    "purgecss": "^1.0",
    "purgecss-webpack-plugin": "^1.5",
    "resolve-url-loader": "^2.3",
    "sass": "^1.20",
    "sass-loader": "7.*",
    "scrolltrigger-classes": "^0.3",
    "stylelint": "^9.6",
    "stylelint-webpack-plugin": "^0.10",
    "svg-spritemap-webpack-plugin": "^3.3",
